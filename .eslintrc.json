{
  "extends": ["plugin:cypress/recommended", "../../../../.eslintrc.json"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {
        "@typescript-eslint/no-var-requires": "off",
        "jest/valid-expect-in-promise": "warn",
        "jest/valid-title": "warn",
        "jest/expect-expect": "off",
        "jest/valid-expect": "off"
      }
    }
  ]
}
